<core:FragmentDefinition xmlns:core="sap.ui.core" 
        xmlns:f="sap.ui.layout.form"
        xmlns="sap.m">
		<f:SimpleForm id="SimpleFormDisplay354"
			editable="false"
			layout="ResponsiveGridLayout"
			title="Details"
			labelSpanXL="3"
			labelSpanL="3"
			labelSpanM="3"
			labelSpanS="12"
			adjustLabelSpan="false"
			emptySpanXL="4"
			emptySpanL="4"
			emptySpanM="4"
			emptySpanS="0"
			columnsXL="1"
			columnsL="1"
			columnsM="1"
			singleContainerFullSize="false" >
			<f:content>
				<Label text="Name" />
				<Text  text="{name}" />
				<Label text="Type" />
				<Text  text="{genus}" />
				<Label text="Price" />
				<Text  text="{price}" />
				<Label text="Calories" />
				<Text  text="{nutritions/calories}" />
			</f:content>
		</f:SimpleForm>
</core:FragmentDefinition>